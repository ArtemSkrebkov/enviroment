{
    "version": 4,
    "include": [
        "CMakePresets.json"
    ],
    "configurePresets": [
        {
            "name": "build-clang-18",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_COMPILER": {
                    "type": "STRING",
                    "value": "clang-18"
                },
                "CMAKE_CXX_COMPILER": {
                    "type": "STRING",
                    "value": "clang++-18"
                },
                "ENABLE_CLANG_FORMAT": false
            }
        },
        {
            "name": "user-vpux-relwithdebinfo",
            "displayName": "user-vpux-relwithdebinfo",
            "inherits": ["build-clang-18", "developer-build-fast-relwithdebinfo"]
        },
        {
            "name": "user-ov-relwithdebinfo",
            "displayName": "user-ov-relwithdebinfo",
            "inherits": ["build-clang-18", "developer-build-fast-relwithdebinfo"],
            "cacheVariables": {
                "ENABLE_DEBUG_CAPS": true,
                "ENABLE_SANITIZER": true
            }
        },
        {
            "name": "user-win-vpux-relwithdebinfo",
            "displayName": "user-win-vpux-relwithdebinfo",
            "inherits": ["developer-build-relwithdebinfo"]
        },
        {
            "name": "user-win-ov-relwithdebinfo",
            "displayName": "user-win-ov-relwithdebinfo",
            "inherits": ["developer-build-relwithdebinfo"],
            "cacheVariables": {
                "ENABLE_DEBUG_CAPS": true
            }
        }
    ]
}
